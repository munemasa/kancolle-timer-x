{
  "manifest_version": 2,
  "name": "KanColle Timer X",
  "version": "0.1",
  "applications": {
    "gecko": {
      "id": "kancolletimerx@miku39.jp",
      "strict_min_version": "57.0"
    }
  },
  "description": "艦これタイマーX",
  "homepage_url": "http://miku39.jp/blog/wp/?page_id=1705",
  "icons": {
    "48": "icons/icon-16.png"
  },
  "permissions": [
    "*://*/kcsapi/*",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "notifications"
  ],
  "options_ui": {
    "page": "options/options.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png"
    },
    "browser_style": true,
    "default_title": "改修工廠カレンダー",
    "default_popup": "popup/powerup.html"
  },
  "sidebar_action": {
    "default_title": "艦これタイマーX",
    "default_panel": "sidebar/sidebar.html",
    "default_icon": {
      "16": "icons/icon-16.png"
    }
  },
  "commands": {
    "_execute_sidebar_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      }
    }
  },
  "background": {
    "scripts": [
      "libs/utils.js",
      "bg/background.js"
    ]
  }
}